bindings {
    nameField: listTextField
    textField: textField
}

types {
    MyItems: list (
        user {
            name: String
        },
        newUser {
            // no fields required
        }
    )
}

inputs {
    items: MyItems
}

layout {
    layer {
        roundRect {
            padding {
                left: test % 2 == 0 ? 20 : nil
                right: test % 2 == 0 ? 20 : nil
            }
            center {
                x : test % 2 == 0 ? nil : 0.5
                y: 0.5
            }
            fixedSize {
                width: test % 2 == 0 ? nil : 120
                height: 80
            }
            backgroundColor: #ffcccc
        }
    }
}
