bindings {
    nameField: listTextField
    textField: textField
}

types {
    MyItems: list (
        user {
            name: String
        },
        newUser {
            // no fields required
        }
    )
}

inputs {
    items: MyItems
}

layout {
    layer {
        textField {
            type: .url
            center { x: 0.5 y: 0.5 }
            strokeColor: #cccccc
            strokeWidth: 5
            cornerRadius: 0.5
            font: Font(18)
        }
    }
}
