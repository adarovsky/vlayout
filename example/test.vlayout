bindings {
}

types {
    TestList: list(
        item {
            name: String
        }
    )
}

inputs {
    counter: Number
    test: String
    testList: TestList
}

properties {
    outerTickerHeadline: "Sample headline"
    outerTickerContent: ""
    session {
        hostIFBJoined: false
        hostIFBOn: false
    }
    topOffset: 0
    bottomOffset: 0
    prefix: ""
}

layout {
    layer {
        id: "test"

        vertical {
            padding { top: 50 left: 16 right: 16 bottom: 50 }

            absolute {
                sizePolicy: .stretched

                label {
                    center { y: 0.5 }
                    padding { left: 30 right: 30 }
                    text: "game.final.question"
                    textColor: #1c192c
                    font: Font("helvetica,sans-serif", 24)
                    textAlignment: .center
                }
            }

            testView {
                aspect: 1
            }
        }
    }
}
